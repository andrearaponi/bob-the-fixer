# SonarGuard Configuration Example
# Copy this file to .env and update with your values
# NOTE: This file is for development reference only
# The MCP server uses environment variables passed via Claude CLI, not this file

SONAR_URL=http://localhost:9000
SONAR_TOKEN=your_sonar_token_here
SONAR_PROJECT_KEY_PREFIX=sonarguard
NODE_ENV=development
LOG_LEVEL=info
TOKEN_ENCRYPTION_KEY=your_64_character_hex_key_here_0123456789abcdef0123456789abcdef
LOG_FORMAT=text
LOG_FILE_PATH=./logs/mcp-server.log